{
  "manifest_version": 2,
  "name": "Tabs Manager",
  "description": "Manager your tabs using shortcuts.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js",
        "script2.js"
      ],
      "run_at": "document_end"
    }
  ],
  "commands": {
    "search-tabs": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Search for Tabs"
    }
  }
}